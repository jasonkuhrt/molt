{
  "name": "molt",
  "repository": "git@github.com:jasonkuhrt/molt.git",
  "author": "Jason Kuhrt",
  "packageManager": "pnpm@8.6.5",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "check:lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "check:types": "tsc --noEmit",
    "release:@molt/command": "pnpm release --package @molt/command",
    "release:@molt/types": "pnpm release --package @molt/types",
    "release:molt": "pnpm release --package molt",
    "release": "ts-node scripts/release.ts"
  },
  "devDependencies": {
    "@octokit/core": "^5.0.0",
    "@prisma-labs/prettier-config": "0.1.0",
    "@swc/core": "1.3.37",
    "@tsconfig/node14": "14.1.0",
    "@tsconfig/node16": "16.1.0",
    "@tsconfig/recommended": "1.0.2",
    "@types/inquirer": "9.0.3",
    "@types/node": "18.14.6",
    "@types/semver": "^7.3.13",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-deprecation": "1.3.3",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "execa": "8.0.1",
    "fast-glob": "3.2.12",
    "fs-jetpack": "5.1.0",
    "markdown-toc": "^1.2.0",
    "nodemon": "^3.0.0",
    "prettier": "3.0.3",
    "semver": "^7.3.8",
    "semver-regex": "4.0.5",
    "ts-node": "10.9.1",
    "typescript": "5.0.2",
    "vitest": "0.29.2",
    "zod": "3.20.6"
  },
  "dependencies": {
    "alge": "0.8.1"
  }
}
