{
  "name": "molt",
  "repository": "git@github.com:jasonkuhrt/molt.git",
  "author": "Jason Kuhrt",
  "packageManager": "pnpm@8.9.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "check:lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "check:types": "tsc --noEmit",
    "release:@molt/command": "pnpm release --package @molt/command",
    "release:@molt/types": "pnpm release --package @molt/types",
    "release:molt": "pnpm release --package molt",
    "release": "tsx scripts/release.ts"
  },
  "devDependencies": {
    "@octokit/core": "^5.0.1",
    "@prisma-labs/prettier-config": "0.1.0",
    "@swc/core": "1.3.93",
    "@tsconfig/node14": "14.1.0",
    "@tsconfig/node16": "16.1.1",
    "@tsconfig/recommended": "1.0.3",
    "@types/inquirer": "9.0.4",
    "@types/node": "20.8.6",
    "@types/semver": "^7.5.3",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-deprecation": "2.0.0",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "execa": "8.0.1",
    "fast-glob": "3.3.1",
    "fs-jetpack": "5.1.0",
    "markdown-toc": "^1.2.0",
    "nodemon": "^3.0.1",
    "prettier": "3.0.3",
    "semver": "^7.5.4",
    "semver-regex": "4.0.5",
    "tsx": "^3.13.0",
    "typescript": "5.2.2",
    "vitest": "0.34.6",
    "zod": "3.22.4"
  },
  "dependencies": {
    "alge": "0.8.1"
  }
}
