{
  "name": "molt",
  "repository": "git@github.com:jasonkuhrt/molt.git",
  "author": "Jason Kuhrt",
  "packageManager": "pnpm@7.13.2",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx --max-warnings 0",
    "types:check": "tsc --noEmit",
    "publish:@molt/command": "ts-node scripts/publish.ts --package @molt/command",
    "publish:@molt/types": "ts-node scripts/publish.ts --package @molt/types",
    "publish:molt": "ts-node scripts/publish.ts --package molt"
  },
  "devDependencies": {
    "@octokit/core": "^4.0.5",
    "@prisma-labs/prettier-config": "0.1.0",
    "@swc/core": "1.3.6",
    "@tsconfig/node14": "1.0.3",
    "@tsconfig/node16": "1.0.3",
    "@tsconfig/recommended": "1.0.1",
    "@types/inquirer": "9.0.2",
    "@types/node": "18.7.17",
    "@types/semver": "^7.3.12",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-deprecation": "1.3.2",
    "eslint-plugin-only-warn": "1.0.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "execa": "6.1.0",
    "fast-glob": "3.2.12",
    "fs-jetpack": "5.0.0",
    "markdown-toc": "^1.2.0",
    "prettier": "2.7.1",
    "semver": "^7.3.7",
    "semver-regex": "4.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "vitest": "0.24.0",
    "zod": "3.19.1"
  }
}
